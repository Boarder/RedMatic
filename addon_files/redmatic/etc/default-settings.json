{
  "uiPort": 1880,
  "uiHost": "127.0.0.1",
  "flowFile": "flows.json",
  "userDir": "/usr/local/addons/redmatic/var",
  "httpRoot": "/addons/red",
  "logging": {
    "ain": {
      "level": "info",
      "metrics": false,
      "audit": false
    }
  },
  "contextStorage": {
    "default": {
      "module": "memory"
    },
    "memory": {
      "module": "memory"
    },
    "file": {
      "module": "localfilesystem",
      "config": {
        "dir": "/usr/local/addons/redmatic/var",
        "flushInterval": 30
      }
    }
  },
  "editorTheme": {
    "projects": {
      "enabled": false
    }
  },
  "adminAuth": {
    "type": "rega",
    "sessionExpiryTime": 86400
  },
  "restartOnCrash": 0
}